{
  "extends": ["airbnb", "plugin:vue/recommended"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "func-names": ["error", "as-needed"],
    "linebreak-style": 0,
    "no-new": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-console": 0,
    "no-restricted-syntax": 0,
    "vue/no-v-html": 0
  },
  "plugins": [
    "vue"
  ]
}